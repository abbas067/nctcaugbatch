@{
    ViewData["Title"] = "Home Page";
}
@model List<nctcaugbatch.Models.EmployeeDetail>
<table border="1" width="400">
    <thead>
        <tr class="btn-primary">
            <th> ID</th>
            <th> Name</th>
            <th> Designation</th>

        </tr>
    </thead>
    <tbody>
        @foreach (var emp in Model)
        {
            <tr>
                <td>@emp.Id</td>
                <td>@emp.Name</td>
                <td>@emp.Designation</td>
            </tr>
        }
    </tbody>
</table>

<div class="text-center">

</div>

